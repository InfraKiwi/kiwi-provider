  "scripts": {
    "cli": "ts-node cmd/kiwiCLI.ts",
    "gen:scripts": "ts-node ./cmd/ci/ciGenHackInstaller.ts",
    "gen:prisma": "prisma generate --schema prisma/schema.prisma",
    "gen:schema": "ts-node ./cmd/ci/ciSchemaGen.ts",
    "gen": "yarn gen:scripts && yarn gen:prisma && yarn gen:schema && yarn tsc:check",
    "test": "jest --config jest.config.cjs",
    "dist:esbuild": "ts-node cmd/esbuild.ts --entryPoint cmd/kiwiCLI.ts --outFile dist/bundle.cjs",
    "fix": "yarn tsc:check && yarn prettier . --write && eslint --fix 'src/**/*.ts' 'cmd/**/*.ts'",
    "lint": "yarn tsc:check && eslint 'src/**/*.ts' 'cmd/**/*.ts'",
    "tsc:check": "tsc --noEmit",
    "test:apk": "docker run -h docker-nginx -i -t --rm --mount type=bind,source=\\\\\\\\\\\\\\\"$(pwd)/\\\\\\\\\\\\\\\",target=\\\\\\\\\\\\\\\"/config\\\\\\\\\\\\\\\" -w /config node:18-alpine node_modules/.bin/ts-node cmd/kiwiCLI.ts examples/recipes/nginx",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma --name init",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma",
    "sass:watch": "sass -w src/app/kiwiProvider/static/scss:src/app/kiwiProvider/static/css",
    "docs:build-docker-linux": "docker build -q -f hack/mkdocs/Dockerfile hack/mkdocs",
    "docs:build-docker-win": "powershell /C '(docker build -q -f hack/mkdocs/Dockerfile hack/mkdocs | Select-String -Pattern \"sha256:\\w+\").Matches[0].ToString().Trim()'",
    "docs:build-docker": "yarn docs:build-docker-$(node hack/os/getWindowsOrLinuxString.mjs)",
    "docs:watch": "docker run --rm -it --entrypoint /bin/sh -p 8000:8000 -v $(pwd):/docs \"$(yarn docs:build-docker)\" -ec 'exec mkdocs serve \"--dev-addr=$(ip route get 1 | awk '\"'{print \\$NF;exit}'\"'):8000\"'",
    "docs:build": "docker run --rm -t --entrypoint /bin/sh -v $(pwd):/docs \"$(yarn docs:build-docker)\" -ec 'set -e; mkdocs build; ls -al /docs/site'",
    "dist:pkg:dev": "ts-node cmd/ci/ciGenArtifactsMatrix.ts --configFile hack/ci/artifacts.dev.yaml --run",
    "dist:version": "ts-node cmd/ci/ciGetBuildVersion.ts --outFile dist/version"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@4.1.0"
