  "scripts": {
    "gen": "prisma generate --schema prisma/schema.prisma && ts-node ./cmd/schemaGen.ts",
    "test": "jest --config jest.config.cjs",
    "dist:esbuild": "ts-node cmd/esbuild.ts",
    "dist:pkg": "ts-node cmd/pkg.ts",
    "pkg": "yarn dist:esbuild && yarn dist:pkg",
    "fix": "yarn prettier . --write && eslint --fix src cmd",
    "lint": "eslint src cmd",
    "test:apk": "docker run -h docker-nginx -i -t --rm --mount type=bind,source=\"$(pwd)/\",target=\"/config\" -w /config node:18-alpine node_modules/.bin/ts-node ./cmd/run.ts examples/recipes/nginx",
    "server:config": "nodemon -- ./cmd/serverConfig.ts -h 0.0.0.0 -i examples/inventory.yaml examples/rb-*",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma --name init",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma",
    "sass:watch": "sass -w src/app/serverConfig/static/scss:src/app/serverConfig/static/css"
  },
