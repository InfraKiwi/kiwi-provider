  "name": "10infra-config",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "gen": "prisma generate --schema prisma/schema.prisma && ts-node ./cmd/schemaGen.ts",
    "test": "jest --config jest.config.cjs",
    "dist:esbuild": "ts-node cmd/esbuild.ts --entryPoint cmd/cli.ts --outFile dist/bundle.cjs",
    "dist:pkg:cli": "ts-node cmd/pkg.ts --outDir dist --entryPoint cmd/cli.ts",
    "fix": "yarn prettier . --write && eslint --fix 'src/**/*.ts' 'cmd/**/*.ts'",
    "lint": "eslint 'src/**/*.ts' 'cmd/**/*.ts'",
    "test:apk": "docker run -h docker-nginx -i -t --rm --mount type=bind,source=\\\\\\\"$(pwd)/\\\\\\\",target=\\\\\\\"/config\\\\\\\" -w /config node:18-alpine node_modules/.bin/ts-node cmd/cli.ts examples/recipes/nginx",
    "server:config": "nodemon -- cmd/configProvider.ts -h 0.0.0.0 -i examples/inventory.yaml examples/rb-*",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema.prisma --name init",
    "prisma:studio": "prisma studio --schema prisma/schema.prisma",
    "sass:watch": "sass -w src/app/configProvider/static/scss:src/app/configProvider/static/css",
    "docs:build-docker-linux": "docker build -q -f hack/mkdocs/Dockerfile hack/mkdocs",
    "docs:build-docker-win": "powershell /C '(docker build -q -f hack/mkdocs/Dockerfile hack/mkdocs | Select-String -Pattern \"sha256:\\w+\").Matches[0].ToString().Trim()'",
    "docs:watch": "docker run --rm -it --entrypoint /bin/sh -p 8000:8000 -v $(pwd):/docs \"$(yarn docs:build-docker-$(node hack/os/getWindowsOrLinuxString.mjs))\" -ec 'exec mkdocs serve \"--dev-addr=$(ip route get 1 | awk '\"'{print \\$NF;exit}'\"'):8000\"'",
    "dist:pkg:agent:win:x64": "ts-node cmd/pkg.ts --outDir dist --entryPoint cmd/agent.ts --nodePlatform win --nodeArch x64",
    "dist:pkg:agent:linux:x64": "ts-node cmd/pkg.ts --outDir dist --entryPoint cmd/agent.ts --nodePlatform linux --nodeArch x64",
    "dist:pkg:agent": "concurrently \"yarn:dist:pkg:agent:*\""
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@types/unzipper": "^0",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
